{
  "cleanUrls": true,
  "trailingSlash": false,
  "rewrites": [
    {
      "source": "/sitemap.xml",
      "destination": "/sitemap.xml"
    },
    {
      "source": "/og-share-image.jpg",
      "destination": "/og-share-image.jpg"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}